{
  "name": "universal-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "build:prod:client": "ng build --prod",
    "build:prod:server": "webpack --config webpack.server.config.js --hide-modules",
    "lint": "ng lint",
    "prebuild": "rimraf dist",
    "prebuild:prod": "rimraf dist",
    "prebuild:prod:client": "rimraf dist/client",
    "prebuild:prod:server": "rimraf dist/server",
    "serve": "ng serve",
    "serve:prod": "npm run build:prod && npm run server",
    "server": "node dist/server/server.js"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-utils/cache": "^2.0.0",
    "@ngx-utils/cookies": "^2.0.1",
    "@ngx-utils/express-engine": "^1.0.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@ngtools/webpack": "^1.4.2",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "codelyzer": "~3.0.1",
    "cssnano": "^3.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webpack": "~2.4.0"
  }
